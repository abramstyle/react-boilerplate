{
  "presets": [
    "react",
    ["env", {
      "targets": {
        "browsers": ["last 2 versions"]
      }
    }],
  ],
  "plugins": [
    "react-hot-loader/babel",
    ["react-css-modules", {
      "generateScopedName": "[name]__[local]___[hash:base64:5]"
    }],
    "syntax-dynamic-import",
    "universal-import"
  ]
}
