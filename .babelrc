{
  "presets": [
    "@babel/react",
    ["@babel/env", {
      "targets": {
        "browsers": ["last 2 versions"]
      }
    }],
  ],
  "plugins": [
    "react-hot-loader/babel",
    "loadable-components/babel",
    "@babel/proposal-class-properties",
    ["react-css-modules", {
      "generateScopedName": "[name]__[local]___[hash:base64:5]"
    }],
  ]
}
